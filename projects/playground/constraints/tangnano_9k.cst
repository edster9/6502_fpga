# Playground Project - Tang Nano 9K Constraints
# Pin assignments for Gowin GW1N-LV9QN88C6/I5

# Clock input - 27MHz crystal
IO_LOC "clk" 52;
IO_PORT "clk" PULL_MODE=UP;

# Reset input - using onboard button
IO_LOC "reset" 3;
IO_PORT "reset" PULL_MODE=UP;

# LED outputs - onboard LEDs
IO_LOC "led[0]" 10;
IO_LOC "led[1]" 11; 
IO_LOC "led[2]" 13;
IO_LOC "led[3]" 14;
IO_LOC "led[4]" 15;
IO_LOC "led[5]" 16;

# Test pin for scope/analyzer
IO_LOC "test_pin" 17;